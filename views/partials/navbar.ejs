<div class="p-3 w-100 bg-primary text-center">
<a href="https://tamudatathon.com" class="text-white">✨&nbsp;Another year, another datathon. Check out our new website to learn more!&nbsp;✨</a>
</div>
<nav class="navbar navbar-expand-lg navbar-dark bg-transparent" style="top: 64px">
  <!-- responsive toggle button -->
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav ml-auto mr-auto pt-2">
      <!-- tell me this is not the most elegant thing ever made -->
      <% req.navbarSamePageItems.forEach(({ name, href, scrollId }) => { %>
      <li class="nav-item active">
        <a class="nav-link same-page-nav-link" href="<%- href %>" scroll-id="<%- scrollId %>">
          <%= name %></a>
      </li>
      <% }) %>
    </ul>

  </div>
</nav>