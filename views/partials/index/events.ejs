<div class="section events-section">
  <div id="event" class="container" style="max-width: 80%">
    <!-- events section title -->
    <div class="row">
      <div class="col-md">
        <h1 class="section-title event-title">
          <ul>
            <li>
              <input type="checkbox" />
              <div>E</div>
            </li>
            <li>
              <input type="checkbox" />
              <div>V</div>
            </li>
            <li>
              <input type="checkbox" />
              <div>E</div>
            </li>
            <li>
              <input type="checkbox" />
              <div>N</div>
            </li>
            <li>
              <input type="checkbox" />
              <div>T</div>
            </li>
            <li>
              <input type="checkbox" />
              <div>S</div>
            </li>
          </ul>
        </h1>
      </div>
    </div>
    <!-- carousel -->
    <div class="event-carousel"> <!-- style="border-style: solid; border-color: #632ed2;"> -->
      <% req.EVENTS.forEach(({ name, description, date }, index) => { %>
      <div class="event-item card">
        <img class="card-img-top" src="/static/img/graphics/carousel-image.png" alt="<%- name %>">
        <div class="card-body">
          <h4 class="card-title">
            <%- name %>
          </h4>
          <b class="card-date">
            <%- date %>
          </b>
          <p class="card-text"><%= description %></p>
          <a href="<%- req.navbarSamePageItems[1].href %>" class="btn btn-primary">
            Go to Event
            <i class="fa fa-external-link"></i>
          </a>
        </div>
      </div>
      <% }) %>
    </div>
    <div class="event-controls">
      <div class="event-prev"></div>
      <div class="event-next"></div>
    </div>
  </div>
</div>