<div class="section sponsor-section">
  <div id="sponsors" class="container">
    <h1 class="section-title sponsors-title">
      Sponsors
    </h1>
    <p class="sponsor-description pb-3">TAMU Datathon would not be possible without our sponsors.
      </br>
      Interested in sponsoring? Check out our
      <a target="_blank" href="/static/resources/datathon_sponsorship_packages_2020.pdf">Sponsorship Packet</a>
      and contact us at
      <a href="mailto:sponsor@tamudatathon.com">sponsor@tamudatathon.com</a>.</p>
    </p>

    <h3>Our Current Sponsors:</h3>
    <div class="sponsor-images">
      <% for (let currentSponsor of req.currentSponsors) {  %>
      <a href="<%= currentSponsor.url %>" target="_blank">
        <img class="sponsor-img <%= currentSponsor.variant %>" src="<%= currentSponsor.img %>" alt="<%= currentSponsor.name %>"></img>
      </a>
      <% } %>
    </div>
    <!-- <h3>Our Past Sponsors:</h3> <div class="sponsor-images"> <% for (let pastSponsor of req.pastSponsors) { %> <a href="<%= pastSponsor.url %>" target="_blank"> <img class="sponsor-img <%= pastSponsor.variant %>" src="<%= pastSponsor.img %>" alt="<%= pastSponsor.name %>"></img> </a> <% } %> </div> -->
  </div>
</div>